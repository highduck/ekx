{
  "name": "@ekx/plugin-firebase",
  "version": "0.0.7",
  "scripts": {
    "postinstall": "cmake-build || exit 0",
    "build_js_pre": "esbuild web/firebase.js --global-name=firebase_js --bundle --sourcemap --target=chrome58,firefox57,safari11,edge16 --outfile=web/dist/firebase.js",
    "build_js_lib": "esbuild web/firebase_c.js --bundle --format=esm --sourcemap --target=node14 --outfile=web/lib/firebase_c.js",
    "build": "yarn run build_js_pre && yarn run build_js_lib"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "CMakeLists.txt",
    "src",
    "java",
    "web/dist",
    "web/lib",
    "ek.js"
  ],
  "license": "ISC",
  "dependencies": {
    "@ekx/app": "0.1.12",
    "cmake-build": "0.0.6",
    "firebase": "^9.8.4"
  }
}
