{
  "name": "@ekx/ekx",
  "version": "0.1.10",
  "license": "ISC",
  "type": "module",
  "main": "modules/cli/main.ts",
  "bin": {
    "ekx": "./modules/cli/main.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "release": "changeset publish",
    "version": "changeset version && yarn run update-lock",
    "update-lock": "yarn && git add yarn.lock",
    "ci:build": "ts-node --esm --swc build.ts",
    "ci:native-build": "ts-node --esm --swc native-build.ts",
    "ci:native-coverage": "ts-node --esm --swc native-coverage.ts",
    "build-external": "cd external && ts-node --esm --swc build.ts",
    "test-tools": "cd tools && ts-node --esm --swc test.ts"
  },
  "dependencies": {
    "@swc/core": "^1.3.11",
    "@types/glob": "^8.0.0",
    "@types/mustache": "^4.2.1",
    "@types/node": "^18.14.0",
    "@types/plist": "^3.0.2",
    "@types/xmldoc": "^1.1.6",
    "colors": "^1.4.0",
    "esbuild": "^0.17.10",
    "glob": "^8.0.3",
    "mustache": "^4.2.0",
    "plist": "^3.0.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "xmldoc": "^1.2.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0"
  }
}
