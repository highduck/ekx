{
  "name": "@ekx/ekx-monorepo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "release": "changeset publish",
    "version": "changeset version && yarn run update-lock",
    "update-lock": "yarn && git add yarn.lock",
    "ci:build": "wsrun -m -e -t build",
    "ci:build-native": "wsrun -m -e -t build:native",
    "ci:test-native": "wsrun -m -e -t test:native",
    "run-test-native": "node run-test-native.js",
    "postinstall": "wsrun -m -e -t postinstall"
  },
  "devDependencies": {
    "@changesets/cli": "^2.23.0",
    "cmake-build": "0.0.6",
    "wsrun": "^5.2.4",
    "@types/xmldoc": "^1.1.6",
    "@types/plist": "^3.0.2",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "xmldoc": "^1.1.4",
    "plist": "^3.0.5"
  },
  "workspaces": {
    "packages": [
      "external/*",
      "packages/*",
      "tools/*",
      "plugins/*",
      "samples/*"
    ]
  }
}
