{
  "name": "@ekx/ekx",
  "description": "Toy game engine",
  "version": "1.0.8",
  "license": "ISC",
  "type": "module",
  "bin": {
    "ekx": "./modules/cli/main.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "ts-node --esm --swc ci/build.ts",
    "build-native": "ts-node --esm --swc ci/build-native.ts",
    "build-native-coverage": "ts-node --esm --swc ci/build-native-coverage.ts",
    "build-external": "ts-node --esm --swc ci/build-external.ts",
    "test-external-utils": "ts-node --esm --swc ci/shdc-test/test.ts",
    "clean": "ts-node --esm --swc ci/clean.ts",
    "update-lock": "yarn && git add yarn.lock",
    "release": "yarn run build && changeset publish",
    "version": "changeset version && yarn run update-lock"
  },
  "dependencies": {
    "@swc/core": "^1.3.36",
    "@types/glob": "^8.1.0",
    "@types/mustache": "^4.2.2",
    "@types/node": "^18.14.0",
    "@types/plist": "^3.0.2",
    "@types/xmldoc": "^1.1.6",
    "colors": "^1.4.0",
    "esbuild": "^0.17.10",
    "glob": "^8.1.0",
    "mustache": "^4.2.0",
    "plist": "^3.0.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "xmldoc": "^1.2.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "engines": {
    "node": ">=18.0.0",
    "yarn": "^1.22.17"
  },
  "files": [
    "tsconfig.json",
    "logo.png",
    "CMakeLists.txt",
    "CHANGELOG.md",
    "tools",
    "packages",
    "packages/auph/web/dist",
    "packages/texture-loader/js",
    "modules",
    "plugins",
    "plugins/firebase/web/dist",
    "plugins/firebase/web/lib",
    "external"
  ]
}
