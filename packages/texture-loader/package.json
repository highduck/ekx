{
  "name": "@ekx/texture-loader",
  "version": "0.0.6",
  "scripts": {
    "postinstall": "cmake-build || exit 0",
    "build_js_pre": "esbuild web/src/index.ts --global-name=TextureLoader --bundle --sourcemap --target=chrome58,firefox57,safari11,edge16 --outfile=js/pre/texture-loader.js",
    "build_js_lib": "esbuild web/src/lib.ts --bundle --format=esm --sourcemap --target=node14 --outfile=js/lib/lib-texture-loader.js",
    "build": "yarn run build_js_pre && yarn run build_js_lib"
  },
  "dependencies": {
    "@ekx/app": "0.1.12",
    "@ekx/core": "0.1.11",
    "@ekx/sokol": "0.0.14"
  },
  "files": [
    "java",
    "js",
    "src",
    "include",
    "CMakeLists.txt",
    "ek.js"
  ]
}
